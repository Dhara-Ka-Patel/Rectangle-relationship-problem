{
	"compilerOptions": {
		"baseUrl": "src",
		"lib": ["dom", "es2017"],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "lib",
		"removeComments": false,
		"rootDir": "src",
		"sourceMap": true,
		"target": "es2017",

		"alwaysStrict": false,
		"noFallthroughCasesInSwitch": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"noImplicitThis": false,
		"noUnusedLocals": false,
		"strictNullChecks": false
	}
}
